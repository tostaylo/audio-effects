{
  "name": "audio-effects",
  "type": "module",
  "version": "0.0.0",
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@playwright/test": "^1.29.2",
    "@types/jest": "^29.0.1",
    "esbuild": "^0.15.6",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "wrangler": "0.0.30"
  },
