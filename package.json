{
  "name": "audio-effects",
  "type": "module",
  "version": "0.0.0",
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@playwright/test": "^1.29.2",
    "@types/jest": "^29.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "esbuild": "^0.15.6",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "wrangler": "0.0.30"
  },
  "private": true,
  "scripts": {
    "build:dev": "npm-run-all --parallel esbuild:dev tailwind:dev tsx:dev",
    "esbuild:dev": "esbuild src/index.jsx --jsx-factory=h --jsx-fragment=Fragment --bundle --outfile=public/js/output.js --watch",
    "tailwind:dev": "npx tailwindcss -i ./src/input.css -o ./public/styles/output.css --watch",
    "tsx:dev": "npx tsc --watch",
    "eslint": "eslint .",
    "lint": "lint-staged",
    "prepare": "husky install",
    "pretty": "npx prettier --write .",
    "publish": "wrangler publish",
    "start": "wrangler dev",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "playwright:test": "npx playwright test"
  },
  "dependencies": {
    "@preact/compat": "^17.1.2",
    "preact": "^10.11.3",
    "react": "npm:@preact/compat",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "npm:@preact/compat",
    "redux": "^4.2.0"
  }
}
